{
  "version": 2,
  "functions": {
    "api/ships.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/water-temperature.ts": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/cache-warmup.ts": {
      "memory": 512,
      "maxDuration": 300
    }
  },
  "crons": [
    {
      "path": "/api/cache-warmup",
      "schedule": "0 6 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        }
      ]
    }
  ]
} 